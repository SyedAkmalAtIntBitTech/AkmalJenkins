<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.intbit.marketing.model.TblEmailDraft" table="tbl_colors" schema="public" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="emailId" type="string">
            <column name="email_id" length="50"/>
        </property>
        <property name="category" type="string">
            <column name="category" length="50"/>
        </property>
        <property name="subCategory" type="string">
            <column name="sub_category" length="50"/>
        </property>
        <property name="subCategoryName" type="string">
            <column name="sub_category_name" length="100"/>
        </property>
        <property name="emailSubject" type="string">
            <column name="email_subject" length="100"/>
        </property>
        <property name="emailAddress" type="string">
            <column name="email_address" length="200"/>
        </property>
        <property name="emailList" type="string">
            <column name="email_list" length="10000"/>
        </property>
        <property name="SessionValue" type="string">
            <column name="session_value" length="10000"/>
        </property>
        <property name="sessionKey" type="string">
            <column name="session_key" length="500"/>
        </property>
        <property name="sessionIframeKey" type="string">
            <column name="session_iframe_key" length="200"/>
        </property>
        <property name="sessionIframeValue" type="string">
            <column name="session_iframe_value" length="10000"/>
        </property>
    </class>
</hibernate-mapping>
